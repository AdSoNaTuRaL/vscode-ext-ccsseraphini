{
  "name": "ccsseraphini",
  "displayName": "cc @sseraphini",
  "description": "VSCode extension that tag cc @sseraphini on Twitter",
  "version": "0.0.1",
  "publisher": "adsonatural",
  "author": {
    "name": "Adson Henrique"
  },
  "icon": "media/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdSoNaTuRaL/vscode-ext-ccsseraphini"
  },
  "bugs": {
    "url": "https://github.com/AdSoNaTuRaL/vscode-ext-ccsseraphini/issues",
    "email": "adsonhenriquesilva@gmail.com"
  },
  "homepage": "",
  "license": "",
  "galleryBanner": {
    "color": "#444cf7",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ccsseraphini.ccsseraphini",
    "onCommand:ccsseraphini.refresh",
    "onView:ccsseraphini-sidebar"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ccsseraphini-view",
          "title": "cc @sseraphini",
          "icon": "media/cc.svg"
        }
      ]
    },
    "views": {
      "ccsseraphini-view": [
        {
          "type": "webview",
          "id": "ccsseraphini-sidebar",
          "name": "Twitter",
          "icon": "media/cc.svg",
          "contextualTitle": "Twitter"
        }
      ]
    },
    "commands": [
      {
        "command": "ccsseraphini.ccsseraphini",
        "category": "Twitter",
        "title": "cc @sseraphini"
      },
      {
        "command": "ccsseraphini.refresh",
        "category": "cc @sseraphini",
        "title": "Refresh"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch --config ./build/node-extension.webpack.config.js\"",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/polka": "^0.5.1",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.31.0",
    "svelte-check": "^1.1.23",
    "svelte-preprocess": "^4.6.1",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  }
}
